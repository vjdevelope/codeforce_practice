#include <iostream>
using namespace std;
int main()
{   
    int n,k=1;
    cin>>n;
    
    int a[n];
    for(int i=1;i<=n;i++)
    {
        cin>>a[i];
    }
    while(k<=n)
    {
     for(int i=1;i<=n;i++)
    {
      if(a[i]==k)
      {
          cout<<i<<" ";
          k++;
      }
    }
    }
    return 0;
}
