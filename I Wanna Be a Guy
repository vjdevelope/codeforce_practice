#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    int p;
    cin>>p;
    int level;
    int a[n+1]={0};
    for(int i=0;i<p;i++)
    {
        cin>>level;
        a[level]=1;
    }
    int q;
    cin>>q;
 
    for(int i=0;i<q;i++)
    {
        cin>>level;
        a[level]=1;
    }

   for(int i=1;i<=n;i++)
   {
       if(a[i]==0)
       {
       cout<<"Oh, my keyboard!";
       return 0;
       }
   }
   cout<<"I become the guy."<<endl;
    return 0;
}
